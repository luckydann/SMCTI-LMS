<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head') %>
    <title>Admin  Page</title>
</head>
<body>
    <div class="admin-panel mx-auto mt-5">
        <a href="/" class="btn btn-secondary my-2">Go Back</a>
        <ul class="nav nav-tabs" id="myTab" role="tablist">
            <li class="nav-item" role="presentation">
                <button class="nav-link active" id="student-tab" data-toggle="tab" data-target="#student" type="button" role="tab" aria-controls="student" aria-selected="true">Manage Students</button>
            </li>
            <li class="nav-item" role="presentation">
                <button class="nav-link" id="teacher-tab" data-toggle="tab" data-target="#teacher" type="button" role="tab" aria-controls="teacher" aria-selected="false">Manage Teachers</button>
            </li>
        </ul>
        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="student" role="tabpanel" aria-labelledby="student-tab">
                <%- include('../partials/manageStudents.ejs') %>
            </div>
            <div class="tab-pane fade" id="teacher" role="tabpanel" aria-labelledby="teacher-tab">
                <%- include('../partials/manageTeachers.ejs') %>
            </div>
        </div>
    </div>
</body>
</html>